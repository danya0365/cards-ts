{
  "name": "@cards-ts/can-i-have-that",
  "version": "0.1.2",
  "description": "Library and console implementation of the game 'Can I Have That' which is a house variation on Continental Rummy.",
  "author": "John Meyer",
  "homepage": "https://github.com/johnameyer/can-i-have-that/tree/master/packages/can-i-have-that#readme",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "spec"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/johnameyer/can-i-have-that.git"
  },
  "bin": {
    "can-i-have-that": "./lib/run.js"
  },
  "scripts": {
    "lint": "tslint src/**/*.ts --force",
    "build": "tsc",
    "clean": "rm -rf lib tsconfig.tsbuildinfo",
    "madge": "npx madge --circular --extensions ts ./src",
    "prestart": "npm run build",
    "start": "node lib/start.js start",
    "debug": "ts-node-dev --rs=false --inspect --transpileOnly src/start.ts start",
    "test": "mocha -r ts-node/register spec/**/*.spec.ts"
  },
  "bugs": {
    "url": "https://github.com/johnameyer/can-i-have-that/issues"
  },
  "dependencies": {
    "@cards-ts/core": "^0.1.1"
  },
